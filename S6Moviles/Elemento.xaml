<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="S6Moviles.Elemento">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Actualizar - Eliminar"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Entry x:Name="txtNombre" ></Entry>
            <Entry x:Name="txtUsuario" ></Entry>
            <Entry x:Name="txtPass" ></Entry>
            <Button x:Name="btnActualizar" Text="Actualizar" Clicked="btnActualizar_Clicked"></Button>
            <Button x:Name="btnEliminar" Text="Eliminar" Clicked="btnEliminar_Clicked"></Button>
        </StackLayout>
        
        
    </ContentPage.Content>
</ContentPage>